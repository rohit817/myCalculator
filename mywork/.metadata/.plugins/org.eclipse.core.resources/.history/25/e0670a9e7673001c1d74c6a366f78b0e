import java.util.*;
public class StringDemo {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str = " Hello";
		String str2 = " Apisero! ";
		System.out.println("Concatination--> "+str.concat(str2));
		System.out.println("Lower_Case--> "+ str.toLowerCase());
		
		
		

	}

}
